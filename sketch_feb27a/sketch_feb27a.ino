void setup() {
  // put your setup code here, to run once:
pinMode(D3, OUTPUT);
}

void loop() {
  digitalWrite(D3, HIGH);
  delay(1500);
  digitalWrite(D3, LOW);
  delay(250);
  digitalWrite(D3, HIGH);
  delay(500);
  digitalWrite(D3, LOW);
  delay(500);
    digitalWrite(D3, HIGH);
  delay(1500);
  digitalWrite(D3, LOW);
  delay(500);
   digitalWrite(D3, HIGH);
   delay(1500);
  digitalWrite(D3, LOW);
  delay(500);
     digitalWrite(D3, HIGH);
   delay(1500);
  digitalWrite(D3, LOW);
  delay(1500);
}
